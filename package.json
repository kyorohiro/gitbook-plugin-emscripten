{
  "title": "Emscripten",
  "name": "gitbook-plugin-emscripten",
  "description": "Insert Emscripten in your pages.",
  "main": "index.js",
  "version": "2.0.0",
  "engines": {
    "gitbook": "*"
  },
  "license": "Apache 2",
  "scripts": {
    "build-js": "gitbook-plugin build ./src/index.js",
    "prepublish": "npm run build-js"
  },
  "keywords": [
    "gitbook",
    "plugin"
  ],
  "gitbook": {
    "blocks": {
      "emscripten": {
          "title": "Emscripten Tag",
          "properties": {
            "src": {
              "type": "string",
              "title": "URL of the video",
              "required": true
            }
          }
      }
    }
  }
}
