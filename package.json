{
  "title": "Emscripten",
  "name": "gitbook-plugin-emscripten",
  "description": "Insert Emscripten in your pages.",
  "main": "index.js",
  "version": "0.0.1",
  "engines": {
    "gitbook": "*"
  },
  "license": "Apache 2",
  "keywords": [
    "gitbook",
    "plugin"
  ],
  "gitbook": {
    "blocks": {
      "emscripten": {
          "title": "Emscripten Tag",
          "properties": {
            "js": {
              "type": "string",
              "title": "xxx.js",
              "required": true
            },
            "mem": {
              "type": "string",
              "title": "xxx.mem",
              "required": true
            }
          }
      }
    }
  }
}
